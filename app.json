{
  "expo": {
    "name": "careme360",
    "slug": "careme360",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon-careme.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "careme",
    "splash": {
      "image": "./assets/images/icon-careme.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-careme.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.andresfvb.careme360",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/images/icon-careme.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
       [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2f2404fc-e5ce-413f-8529-53a64b3c89f6"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2f2404fc-e5ce-413f-8529-53a64b3c89f6"
    }
  }
}
